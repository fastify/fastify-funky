{
  "name": "fastify-funky",
  "description": "Support for fastify routes returning functional structures, such as fp-ts Either, Task, TaskEither or plain javascript parameterless functions",
  "version": "1.0.2",
  "license": "MIT",
  "maintainers": [
    {
      "name": "Igor Savin",
      "email": "kibertoad@gmail.com"
    }
  ],
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "jest --config=jest.config.json",
    "test:coverage": "jest --config=jest.config.json --coverage",
    "test:ci": "npm run lint && npm run test:typescript && npm run test:coverage",
    "test:typescript": "tsd",
    "lint": "eslint --format codeframe \"lib/**/*.js\" \"test/**/*.js\" index.js",
    "prettier": "prettier --write \"{lib,test}/**/*.js\" index.js index.d.ts",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "dependencies": {
    "fastify-plugin": "^3.0.0"
  },
  "peerDependencies": {
    "fastify": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^15.0.3",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "fastify": "^3.15.1",
    "fp-ts": "^2.10.5",
    "husky": "^6.0.0",
    "jest": "^27.0.1",
    "pinst": "^2.1.6",
    "prettier": "~2.3.0",
    "tsd": "^0.17.0",
    "typescript": "4.3.2"
  },
  "engines": {
    "node": ">=10.16.0"
  },
  "homepage": "https://github.com/fastify/fastify-funky",
  "repository": {
    "type": "git",
    "url": "git://github.com/fastify/fastify-funky.git"
  },
  "keywords": [
    "fastify",
    "plugin",
    "typescript",
    "algebraic-data-types",
    "functional-programming",
    "functional",
    "fp-ts",
    "either",
    "task"
  ],
  "files": [
    "README.md",
    "LICENSE",
    "lib/*",
    "index.js",
    "index.d.ts"
  ]
}
