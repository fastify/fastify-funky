{
  "name": "fastify-funky",
  "description": "Support for fastify routes returning functional structures, such as fp-ts Either, Task, TaskEither or plain javascript parameterless functions",
  "version": "1.0.1",
  "license": "MIT",
  "maintainers": [
    {
      "name": "Igor Savin",
      "email": "kibertoad@gmail.com"
    }
  ],
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "jest --config=jest.config.json",
    "test:coverage": "jest --config=jest.config.json --coverage",
    "test:ci": "npm run lint && npm run test:typescript && npm run test:coverage",
    "test:typescript": "tsd",
    "lint": "eslint --format codeframe \"lib/**/*.js\" \"test/**/*.js\" index.js",
    "prettier": "prettier --write \"{lib,test}/**/*.js\" index.js index.d.ts"
  },
  "dependencies": {
    "fastify-plugin": "^3.0.0"
  },
  "peerDependencies": {
    "fastify": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^15.0.1",
    "fastify": "^3.9.1",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.2.0",
    "fp-ts": "^2.9.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "tsd": "^0.14.0",
    "typescript": "4.2.3"
  },
  "engines": {
    "node": ">=10.16.0"
  },
  "homepage": "https://github.com/fastify/fastify-funky",
  "repository": {
    "type": "git",
    "url": "git://github.com/fastify/fastify-funky.git"
  },
  "keywords": [
    "fastify",
    "plugin",
    "typescript",
    "algebraic-data-types",
    "functional-programming",
    "functional",
    "fp-ts",
    "either",
    "task"
  ],
  "files": [
    "README.md",
    "LICENSE",
    "lib/*",
    "index.js",
    "index.d.ts"
  ]
}
