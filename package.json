{
  "name": "@fastify/funky",
  "description": "Support for fastify routes returning functional structures, such as fp-ts Either, Task, TaskEither or plain javascript parameterless functions",
  "version": "2.0.0",
  "license": "MIT",
  "maintainers": [
    {
      "name": "Igor Savin",
      "email": "kibertoad@gmail.com"
    }
  ],
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "npm run test:unit && npm run test:typescript",
    "test:unit": "jest --config=jest.config.json --coverage",
    "test:typescript": "tsd",
    "lint": "eslint \"test/**/*.js\" index.js",
    "prettier": "prettier --write \"{lib,test}/**/*.js\" index.js index.d.ts"
  },
  "dependencies": {
    "fastify-plugin": "^4.0.0"
  },
  "peerDependencies": {
    "fastify": "^3.0.0"
  },
  "devDependencies": {
    "@fastify/pre-commit": "2.0.2",
    "@types/node": "^18.0.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fastify": "^4.2.0",
    "fp-ts": "^2.11.2",
    "jest": "^29.0.1",
    "prettier": "^2.4.1",
    "tsd": "^0.20.0"
  },
  "homepage": "https://github.com/fastify/fastify-funky",
  "repository": {
    "type": "git",
    "url": "git://github.com/fastify/fastify-funky.git"
  },
  "keywords": [
    "fastify",
    "plugin",
    "typescript",
    "algebraic-data-types",
    "functional-programming",
    "functional",
    "fp-ts",
    "either",
    "task"
  ],
  "files": [
    "README.md",
    "LICENSE",
    "lib/*",
    "index.js",
    "index.d.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "pre-commit": [
    "lint",
    "test"
  ]
}
